<body class="container">
    <form class="form" action="/users/login" method="POST" enctype="application/x-www-form-urlencoded">
        <fieldset>
            <legend>Log In Information</legend>
            <input class="forms" type="text" name="username" required placeholder="User Name" />
            <br>
            <input class="forms" type="password" name="password" required placeholder="Password" />
        </fieldset>
        <br>
        <input class="sub" type="submit" value="Log In">
        <br><br>
        <a class="TOSLink" href="/registration"> Don't Have an Account? Register Here!</a>
    </form>
</body>